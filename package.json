    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "hbs": "~3.1.0",
    "morgan": "~1.6.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
